{
  "compilerOptions": {
    "target": "es2019",
    "module": "CommonJS",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": ".",
    "paths": {
      "/*": ["web/*"]
    },
    "outDir": "dist",
    "inlineSourceMap": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  }
}
